{
	"version": 2,
	"builds": [
		{
		"src": "The_conversor/wsgi.py",
		"use": "@vercel/python",
		"config": { "maxLambdaSize": "15mb", "runtime": "python3.10"}
		},
	{
		"src": "build.sh",
		"use": "@vercel/python",
		"config": {"distDir": "static"}
	}
	],
	"routes": [
		{
		"src": "/static/(.*)",
		"dest": "/static/$1"
		},	
		{
		"src": "/(.*)",
		"dest": "The_conversor/wsgi.py"
		}
	],
	"outputDirectory": "static"
}